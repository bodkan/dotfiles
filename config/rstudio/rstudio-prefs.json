{
    "save_workspace": "never",
    "load_workspace": false,
    "remove_history_duplicates": true,
    "posix_terminal_shell": "bash",
    "relative_line_numbers": true,
    "highlight_selected_line": true,
    "panes": {
        "quadrants": [
            "Console",
            "TabSet2",
            "Source",
            "TabSet1"
        ],
        "tabSet1": [
            "Environment",
            "Connections",
            "Build",
            "Presentation"
        ],
        "tabSet2": [
            "History",
            "Files",
            "Plots",
            "Packages",
            "Help",
            "VCS",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "Tutorial"
        ],
        "console_left_on_top": true,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "editor_keybindings": "vim",
    "strip_trailing_whitespace": true,
    "syntax_color_console": true,
    "rainbow_parentheses": true,
    "rmd_chunk_output_inline": false,
    "terminal_bell_style": "none",
    "editor_scroll_multiplier": 200,
    "rmd_viewer_type": "pane",
    "save_files_before_build": true,
    "toolbar_visible": false,
    "show_panel_focus_rectangle": true,
    "always_save_history": false,
    "python_project_environment_automatic_activate": false,
    "terminal_python_integration": false,
    "jobs_tab_visibility": "shown",
    "default_project_location": "/private/tmp",
    "initial_working_directory": "~",
    "sync_files_pane_working_dir": true
}